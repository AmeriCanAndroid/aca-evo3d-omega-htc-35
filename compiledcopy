#cleanup
if [ -e ../compiled ];
then
rm -r ../compiled;
fi;

#packageprep
if [ ! -e ../ACAKernels ]; then mkdir ../ACAKernels; fi;

if [ ! -e ../compiled ]; then
mkdir ../compiled ../compiled/modules;
fi;

#packaging
cp ./arch/arm/boot/zImage ../compiled/zImage
find ./ -type f -name "*.ko" -exec cp {} ../compiled/modules/ \;


